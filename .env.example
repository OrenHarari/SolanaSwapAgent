# Solana Swap Agent Configuration

# Solana Network Configuration
SOLANA_RPC_URL=https://api.devnet.solana.com
SOLANA_WS_URL=wss://api.devnet.solana.com
SWAP_AGENT_PROGRAM_ID=BPF1111111111111111111111111111111111111111

# Wallet Configuration
# WARNING: Never commit real private keys to version control
# Generate with: solana-keygen new --no-bip39-passphrase
WALLET_PRIVATE_KEY=your_base58_private_key_here

# Trading Parameters
MIN_PROFIT_THRESHOLD=0.01
MAX_SLIPPAGE_BPS=50
MAX_POSITION_SIZE=1.0
PRICE_UPDATE_INTERVAL=0.1
MAX_CONCURRENT_SWAPS=5

# DEX API Configuration
JUPITER_API_URL=https://quote-api.jup.ag/v6
RAYDIUM_API_URL=https://api.raydium.io/v2

# Infrastructure
REDIS_URL=redis://localhost:6379
LOG_LEVEL=INFO
LOG_FORMAT=json

# MEV Protection (Optional)
USE_JITO_BUNDLES=false
JITO_TIP_AMOUNT=0.001

# Monitoring
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000